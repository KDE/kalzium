#. i18n: tag label attribute value
#: element_tiny.xml:103
#, kde-format
msgid "Antimony"
msgstr "Antimoon"

#: element_tiny.xml:104
#, kde-format
msgid "Arabic 'anthos ammonos' for 'blossom of the god Ammon'."
msgstr "Arabisch 'anthos ammonos' voor 'bloesem van de god Ammon'."

#. i18n: tag label attribute value
#: element_tiny.xml:105
#, kde-format
msgid "Tellurium"
msgstr "Telluur"

#: element_tiny.xml:106
#, kde-format
msgid "Latin 'tellus' or 'telluris' for 'Planet Earth'."
msgstr "Latijn 'tellus' of 'telluris' voor 'planeet Aarde'."

#. i18n: tag label attribute value
#: element_tiny.xml:107
#, kde-format
msgid "Iodine"
msgstr "Jodium"

