#. i18n: tag label attribute value
#: element_tiny.xml:43
#, kde-format
msgid "Scandium"
msgstr "ਸਕੈਡੀਅਮ"

#: element_tiny.xml:44
#, kde-format
msgid "Named because it was found in Scandinavia."
msgstr ""

#. i18n: tag label attribute value
#: element_tiny.xml:45
#, kde-format
msgid "Titanium"
msgstr "ਟਟੇਨੀਅਮ"

#: element_tiny.xml:46
