msgstr "Uhlík"

#: element_tiny.xml:14
#, kde-format
msgid "Latin 'carboneum' for carbon."
msgstr "Latinsky 'carboneum' pre uhlík."

#. i18n: tag label attribute value
#: element_tiny.xml:15
#, kde-format
msgid "Nitrogen"
